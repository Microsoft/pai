<table class="table table-hover">
  <thead>
    <tr>
      <th>JobName</th>
      <th>Username</th>
      <th>JobStartTime</th>
      <th>ElapsedTime</th>
      <th>Retries</th>
      <th>Status</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < jobs.length; i ++) { %>
      <tr>
        <td><a href="#" onclick="loadJobDetail('<%= jobs[i].name %>')"><%= jobs[i].name %></a></td>
        <td><%= jobs[i].username %></td>
        <td><%= convertTime(false, jobs[i].createdTime) %></td>
        <td><%= convertTime(true, jobs[i].createdTime, jobs[i].completedTime) %></td>
        <td><%= jobs[i].retries %></td>
        <td><%= convertState(jobs[i].state, jobs[i].appExitType) %></td>
        <td><button class="btn btn-default btn-sm" onclick="deleteJob('<%= jobs[i].name %>')">Delete</button></td>
      </tr>
    <% } %>
  </tbody>
</table>