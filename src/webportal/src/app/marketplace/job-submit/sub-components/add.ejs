<!-- Modal -->
<div id="addModal" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" id="btn-close-add-modal" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title">Add a new <%= type %></h4>
        </div>
        <div class="modal-body">
          <div id="itemPlaceHolder">
            <div class="panel panel-default shadow">
              <div class="panel-heading"></div>
              <div id="collapse-<%= type %>" class="panel-collapse collapse in">
                <div class="row component">
                  <div class="col-md-2 component-image">
                    <img src="/assets/img/<%= type %>.png" alt="model-logo" class="img-responsive"></img>
                  </div>
                  <div class="tab-content col-md-10" id="<%= type %>-summary">
                      <%= summaryLayout({'name': name, 'type':type, 'id':id, 'contributor':contributor,  'description': description}) %>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <h2>Recommand</h2>
          <div id="recommandPlaceHolder" style="height:250px"></div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-primary" id="btn-add-modal">Add</button>
        </div>
      </div>
    </div>
  </div>
  <div id="editPlaceHolder"></div>